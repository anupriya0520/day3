class Main {
    public static void main(String[] args) {
        for(int n=1;n<=50;n++){
            int k=0;
            int i=1;
            while(i<=n){
            if(n%i==0){
                k++;
            }
            i++;
            }
        if(k==2){
            System.out.println(n+" "+"prime");
        } 
        }
    }
}
