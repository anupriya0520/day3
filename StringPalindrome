class Main {
    public static void main(String[] args) {
        String st="madam";
        String str="";
        for(int i=st.length()-1;i>=0;i--){
            str=str+st.charAt(i);
        }
        if(str==st){
            System.out.println("palindrome");
        }else{
            System.out.println("not a palindrome");
        }
    }
}
