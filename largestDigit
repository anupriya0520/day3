class Main {
    public static void main(String[] args) {
        int n=12376854;
        int maxDigit=0;
        while(n>0){
            int digit=n%10;
            if(digit>maxDigit){
                maxDigit=digit;
            }
            n/=10;
        }
        System.out.println(maxDigit);
    }
}
